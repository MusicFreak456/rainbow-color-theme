{
	"name": "Rainbow Color Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#202020",
		"editor.foreground": "#F9FAF4",
		"editor.foldBackground": "#484848",
		"editor.selectionBackground": "#3E515D",
		"editor.selectionForeground": "#44CEEF",
		"editor.lineHighlightBackground": "#31312A",
		"editorCursor.foreground": "#BBBBBB",
		"sideBar.background": "#202020",
		"sideBarTitle.foreground": "#F9FAF4",
	},
	"tokenColors": [
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#79ABFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Enum Member, Constant",
			"scope": [
				"variable.other.enummember",
				"variable.other.global"
			],
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#626262",
				"fontStyle": ""
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"variable.other.property.cli",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#66E1F8",
				"fontStyle": ""
			}
		},
		{
			"name": "Static Properties",
			"scope": [
				"variable.other.property.static"
			],
			"settings": {
				"foreground": "#8DDAF8",
				"fontStyle": ""
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.other.unit",
				"keyword.other",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.new",
				"constant.language",
				"entity.name.function.preprocessor",
				"variable.language"
			],
			"settings": {
				"foreground": "#DD2867",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"entity.name.label",
				"support.constant",
				"keyword",
			],
			"settings": {
				"foreground": "#F9FAF4",
				"fontStyle": ""
			}
		},
		{
			"name": "Constant Character",
			"scope": [
				"constant.character",
				"constant.character.escape",
				"constant.other"
			],
			"settings": {
				"foreground": "#AE81FF",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#F07178",
				"fontStyle": ""
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"meta.function entity.name.function"
			],
			"settings": {
				"foreground": "#1EB540",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"meta.function-call entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.function.templated"
			],
			"settings": {
				"foreground": "#A7EC21",
				"fontStyle": ""
			}
		},
		{
			"name": "Static Function Call",
			"scope": [
				"meta.function-call entity.name.function.member.static"
			],
			"settings": {
				"foreground": "#96EC3F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"variable",
				"meta.block variable.other",
				"variable.other.local"
			],
			"settings": {
				"foreground": "#FFBF26",
				"fontStyle": ""
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FFBF26",
				"fontStyle": ""
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#6897BB",
				"fontStyle": ""
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#17C6A3",
				"fontStyle": ""
			}
		},
		{
			"name": "Class, Support, Namespace",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.class.generic",
				"entity.name.type.class.templated",
				"entity.name.namespace",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#1290C3",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6",
				"fontStyle": ""
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#FF5370",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#3EABE6",
				"fontStyle": "bold"
			}
		}
	]
}
