{
	"name": "Rainbow Color Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#202020",
		"editor.foreground": "#F9FAF4",
		"editor.foldBackground": "#484848",
		"editor.selectionBackground": "#3E515D",
		"editor.selectionForeground": "#44CEEF",
		"editor.lineHighlightBackground": "#31312A",
		"editorCursor.foreground": "#BBBBBB",
		"sideBar.background": "#202020",
		"sideBarTitle.foreground": "#F9FAF4",
	},
	"tokenColors": [
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#79ABFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Enum Member, Constant",
			"scope": [
				"variable.other.enummember",
				"variable.other.global"
			],
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#626262"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"variable.other.property.cli",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#66E1F8"
			}
		},
		{
			"name": "Static Properties",
			"scope": [
				"variable.other.property.static"
			],
			"settings": {
				"foreground": "#8DDAF8",
				"fontStyle": ""
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.other.unit",
				"keyword.other",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.new",
				"constant.language",
				"entity.name.function.preprocessor",
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DD2867"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"entity.name.label",
				"support.constant",
				"keyword",
			],
			"settings": {
				"foreground": "#F9FAF4"
			}
		},
		{
			"name": "Constant Character",
			"scope": [
				"constant.character",
				"constant.character.escape",
				"constant.other"
			],
			"settings": {
				"foreground": "#AE81FF",
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#F07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"meta.function entity.name.function"
