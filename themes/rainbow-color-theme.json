{
	"name": "Rainbow Color Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#202020",
		"editor.foreground": "#F9FAF4",
		"editor.foldBackground": "#484848",
		"editor.selectionBackground": "#3E515D",
		"editor.selectionForeground": "#44CEEF",
		"editor.lineHighlightBackground": "#31312A",
		"editorCursor.foreground": "#BBBBBB"
	},
	"tokenColors": [
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter",
			],
			"settings": {
				"foreground": "#79ABFF",
				"fontStyle": ""
			}
		},
		{
			"name": "Enum Member, Constant",
			"scope": [
				"variable.other.enummember",
				"variable.other.global"
			],
			"settings": {
				"foreground": "#66D9EF",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#626262",
				"fontStyle": ""
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"variable.other.property.cli",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#66E1F8",
				"fontStyle": ""
			}
		},
		{
			"name": "Static Properties",
			"scope": [
				"variable.other.property.static"
			],
			"settings": {
				"foreground": "#8DDAF8",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.other.unit",
				"keyword.other",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.new",
				"constant.language",
				"entity.name.function.preprocessor",
				"variable.language"
			],
			"settings": {
				"foreground": "#DD2867",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.section.embedded",
				"entity.name.label",
				"support.constant",
				"keyword",
			],
			"settings": {
				"foreground": "#F9FAF4",
				"fontStyle": ""
			}
		},
		{
			"name": "Constant Character",
			"scope": [
				"constant.character",
				"constant.character.escape",
				"constant.other"
			],
			"settings": {
				"foreground": "#AE81FF",
				"fontStyle": ""
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"meta.function-call entity.name.function",
				"entity.name.function.member",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.function.templated"
			],
			"settings": {
				"foreground": "#A7EC21",
				"fontStyle": ""
			}
		},
		{
			"name": "Static Function Call",
			"scope": [
				"meta.function-call entity.name.function.member.static"
			],
			"settings": {
				"foreground": "#96EC3F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Declaration",
			"scope": [
				"meta.function entity.name.function"
			],
			"settings": {
				"foreground": "#1EB540",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Local Variables",
			"scope": [
				"variable.other.local",
				"support.other.variable",
			],
			"settings": {
				"foreground": "#FFBF26",
				"fontStyle": ""
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#6897BB",
				"fontStyle": ""
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key"
			],
			"settings": {
				"foreground": "#17C6A3",
				"fontStyle": ""
			}
		},
		{
			"name": "Class, Type, Namespace",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.class.generic",
				"entity.name.type.class.templated",
				"entity.name.type.class.reference",
				"entity.name.type.class.value",
				"entity.name.namespace",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#1290C3",
				"fontStyle": "bold"
			}
		}
	]
}
